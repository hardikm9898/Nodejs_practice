<%-include("../include/head.ejs")%>
<link rel="stylesheet" href="http://localhost:<%=process.env.PORT%>/product.css">
<link rel="stylesheet" href="http://localhost:<%=process.env.PORT%>/main.css">
</head>
<body>
<%-include("../include/navigation.ejs")%>

<div class="grid">
 
        <article class="card product-item">
            <header class="card__header">
                <h1 class="product__title"><%= prods.username %></h1>
            </header>
            <hr>
            <div class="card__image">
                <img src="data:image/jpeg;base64,"
                    alt="A Book">
            </div>
            <div class="card__content">
                <hr>
                <h2 class="product__price"><%= prods.userid %></h2>
                <hr>
                <hr>
                <p class="product__description">A very nice!</p>
            </div>
     
        </article>

</div>


<%-include("../include/end.ejs")%>